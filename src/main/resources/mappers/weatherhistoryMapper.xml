<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

 <mapper namespace="com.hyungeerhee.spring.test.jstl.repository.WeatherHistoryRepository">
 
	<select id="selectWeatherHIstory"
	resultType="com.hyungeerhee.spring.test.jstl.domain.Weather">
		SELECT 
			`id`
			, `weather`
			, `date`
			, `temperatures`
			, `precipitation`
			, `microDust`
			, `windSpeed`
			, `createdAt`
			, `updatedAt`
		FROM
			`weatherhistory`
		
	</select>
  
 </mapper>